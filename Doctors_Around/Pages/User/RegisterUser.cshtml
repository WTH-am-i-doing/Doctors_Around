@page
@model Doctors_Around.Pages.User.RegisterUserModel
@{
    ViewData["Title"] = "RegisterUser";
}




    <section class="container-fluid">
        <section class="row justify-content-center">
            <section class="col-10 col-sm-12 col-md-6">
                <form class="form-container-custom">
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label for="inputEmail4">Email</label>
                            <input type="email" class="form-control" id="inputEmail4" placeholder="xyz@domain.com">
                        </div>
                        <div class="form-group col-md-6">
                            <label for="inputPassword4">Password</label>
                            <input type="password" class="form-control" id="inputPassword4" placeholder="password ">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputAddress">Full Name</label>
                        <input type="text" class="form-control" id="inputAddress">
                    </div>
                    <div class="form-group">
                        <label for="inputAddress2">Phone Number</label>
                        <input type="text" class="form-control" id="inputAddress2" placeholder="+XXX YYYYYYYYY">
                    </div>
                    <button type="submit" class="btn btn-primary btn-block">Sign Up</button>
                </form>
            </section>
        </section>
    </section>

